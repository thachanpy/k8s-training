apiVersion: v1
kind: Pod
metadata:
  labels:
    color: blue
  name: configmap-pod-2
  namespace: dev-namespace
spec:
  containers:
  - image: nginx
    name: configmap-container
    env:
    - name: APP_USERNAME_XX
      valueFrom:
        configMapKeyRef:
          name: env-config          
          key: app_username
